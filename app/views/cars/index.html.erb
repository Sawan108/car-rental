<br>

<div class="cards">
  <% @cars.each do |car| %>
    <div class="card-trip">
      <img src="https://source.unsplash.com/random/240x300/?<%= car.model %>">
      <div class="card-trip-infos">
        <div>
          <h2><%= car.model %></h2>
          <p></p>
        </div>
        <h2 class="card-trip-pricing"><%= car.price %></h2>
      </div>
      <div>
        <%= link_to "Book now", car_path(car), class: "btn btn-flat" %>
      </div>
    </div>
  <% end %>
</div>
